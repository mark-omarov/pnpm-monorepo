{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "prepare-deploy": {
      "dependsOn": ["build", "lint", "test"],
      "outputs": ["deployment/**"]
    }
  }
}
